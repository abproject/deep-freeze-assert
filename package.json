{
  "name": "deep-freeze-assert",
  "version": "1.0.8",
  "description": "A Node.js module for listening to attempts of mutation objects and arrays.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --collectCoverage",
    "lint": "tslint --config ./tslint.json --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abproject/deep-freeze-assert.git"
  },
  "author": "Anton Bezpalov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abproject/deep-freeze-assert/issues"
  },
  "homepage": "https://github.com/abproject/deep-freeze-assert#readme",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "coveralls": "^3.0.0",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
